<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shangfang.dao.ManagerDao">
    <select id="selectManager" resultType="com.shangfang.domain.Manager">
        select name,password from manager where name=#{name} and password=#{password}
    </select>
</mapper>
        <!--指定约束文件-->